{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "datasource list",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true,
      "args": ["datasource", "list"]
    },
    {
      "name": "datasource create",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true,
      "args": ["datasource", "create", "-name", "golive", "-accountName", "golivemisc", "-containerName", "sigmf-metadata", "-description", "my sigmf metadata"]
    },
    {
      "name": "metadata addfolder",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true,
      "args": ["metadata", "addfolder", "-accountName", "golivemisc", "-containerName", "sigmf-metadata"]
    },
    {
      "name": "metadata list",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true,
      "args": ["metadata", "list", "-accountName", "golivemisc", "-containerName", "sigmf-metadata"]
    },
    {
      "type": "msedge",
      "request": "launch",
      "name": "Launch Edge against localhost",
      "url": "http://localhost:3001",
      "webRoot": "${workspaceFolder}"
    }
  ]
}